        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    game:GetService("UserInputService").InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input)
        end
    end)
    
    -- Title
    Title.Name = "Title"
    Title.Parent = TopBar
    Title.BackgroundTransparency = 1
    Title.Position = UDim2.new(0, 10, 0, 0)
    Title.Size = UDim2.new(1, -40, 1, 0)
    Title.Font = Enum.Font.GothamMedium
    Title.Text = "Steal System"
    Title.TextColor3 = Color3.fromRGB(200, 200, 200)
    Title.TextSize = 11
    Title.TextXAlignment = Enum.TextXAlignment.Left
    
    -- CloseBtn
    CloseBtn.Name = "CloseBtn"
    CloseBtn.Parent = TopBar
    CloseBtn.BackgroundTransparency = 1
    CloseBtn.Position = UDim2.new(1, -25, 0, 0)
    CloseBtn.Size = UDim2.new(0, 25, 0, 25)
    CloseBtn.Font = Enum.Font.GothamBold
    CloseBtn.Text = "Ã—"
    CloseBtn.TextColor3 = Color3.fromRGB(180, 180, 180)
    CloseBtn.TextSize = 18
    
    -- Container
    Container.Name = "Container"
    Container.Parent = MainFrame
    Container.BackgroundTransparency = 1
    Container.Position = UDim2.new(0, 0, 0, 30)
    Container.Size = UDim2.new(1, 0, 1, -35)
    
    -- CreatorStealBtn
    CreatorStealBtn.Name = "CreatorStealBtn"
    CreatorStealBtn.Parent = Container
    CreatorStealBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    CreatorStealBtn.BackgroundTransparency = 0.2
    CreatorStealBtn.BorderSizePixel = 0
    CreatorStealBtn.Position = UDim2.new(0.08, 0, 0.15, 0)
    CreatorStealBtn.Size = UDim2.new(0.84, 0, 0, 26)
    CreatorStealBtn.Font = Enum.Font.GothamMedium
    CreatorStealBtn.Text = "Creator Steal"
    CreatorStealBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    CreatorStealBtn.TextSize = 11
    CreatorStealBtn.AutoButtonColor = false
    
    local CreatorCorner = Instance.new("UICorner")
    CreatorCorner.CornerRadius = UDim.new(0, 6)
    CreatorCorner.Parent = CreatorStealBtn
    
    local CreatorStroke = Instance.new("UIStroke")
    CreatorStroke.Color = Color3.fromRGB(90, 90, 90)
    CreatorStroke.Thickness = 1
    CreatorStroke.Transparency = 0.5
    CreatorStroke.Parent = CreatorStealBtn
    
    -- InstantStealBtn
    InstantStealBtn.Name = "InstantStealBtn"
    InstantStealBtn.Parent = Container
    InstantStealBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    InstantStealBtn.BackgroundTransparency = 0.2
    InstantStealBtn.BorderSizePixel = 0
    InstantStealBtn.Position = UDim2.new(0.08, 0, 0.45, 0)
    InstantStealBtn.Size = UDim2.new(0.84, 0, 0, 26)
    InstantStealBtn.Font = Enum.Font.GothamMedium
    InstantStealBtn.Text = "Instant Steal"
    InstantStealBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    InstantStealBtn.TextSize = 11
    InstantStealBtn.AutoButtonColor = false
    
    local InstantCorner = Instance.new("UICorner")
    InstantCorner.CornerRadius = UDim.new(0, 6)
    InstantCorner.Parent = InstantStealBtn
    
    local InstantStroke = Instance.new("UIStroke")
    InstantStroke.Color = Color3.fromRGB(90, 90, 90)
    InstantStroke.Thickness = 1
    InstantStroke.Transparency = 0.5
    InstantStroke.Parent = InstantStealBtn
    
    -- StatusDot
    StatusDot.Name = "StatusDot"
    StatusDot.Parent = Container
    StatusDot.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    StatusDot.BorderSizePixel = 0
    StatusDot.Position = UDim2.new(0.08, 0, 0.85, 0)
    StatusDot.Size = UDim2.new(0, 6, 0, 6)
    
    local DotCorner = Instance.new("UICorner")
    DotCorner.CornerRadius = UDim.new(1, 0)
    DotCorner.Parent = StatusDot
    
    -- StatusLabel
    StatusLabel.Name = "StatusLabel"
    StatusLabel.Parent = Container
    StatusLabel.BackgroundTransparency = 1
    StatusLabel.Position = UDim2.new(0.08, 12, 0.82, 0)
    StatusLabel.Size = UDim2.new(0.84, -12, 0, 12)
    StatusLabel.Font = Enum.Font.Gotham
    StatusLabel.Text = "Ready"
    StatusLabel.TextColor3 = Color3.fromRGB(140, 140, 140)
    StatusLabel.TextSize = 9
    StatusLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    -- FunÃ§Ã£o Creator Steal
    local function CreatorSteal()
        local character = LocalPlayer.Character
        if not character or not character:FindFirstChild("HumanoidRootPart") then
            StatusLabel.Text = "Character not found"
            TweenService:Create(StatusLabel, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(255, 100, 100)}):Play()
            TweenService:Create(StatusDot, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(255, 100, 100)}):Play()
            wait(2)
            StatusLabel.Text = "Ready"
            TweenService:Create(StatusLabel, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(140, 140, 140)}):Play()
            TweenService:Create(StatusDot, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(100, 100, 100)}):Play()
            return
        end
        
        local hrp = character.HumanoidRootPart
        
        -- Teleportar para a parte
        hrp.CFrame = creatorPart.CFrame + Vector3.new(0, 3, 0)
        
        StatusLabel.Text = "Teleported!"
        TweenService:Create(StatusLabel, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(100, 255, 100)}):Play()
        TweenService:Create(StatusDot, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(100, 255, 100)}):Play()
        
        wait(2)
        StatusLabel.Text = "Ready"
        TweenService:Create(StatusLabel, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(140, 140, 140)}):Play()
        TweenService:Create(StatusDot, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(100, 100, 100)}):Play()
    end
    
    -- Efeitos hover Creator Steal
    CreatorStealBtn.MouseEnter:Conne
